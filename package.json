{
  "name": "11ty-snowpack-tailwind-starter",
  "version": "0.0.1",
  "description": "11ty + Snowpack + tailwindcss",
  "private": true,
  "engines": {
    "node": "12.x.x",
    "npm": "6.x.x"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "11ty",
    "snowpack",
    "tailwindcss"
  ],
  "author": {
    "name": "Florian Bacher",
    "email": "florian.bacher@lean-coders.at"
  },
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "start": "cross-env NODE_ENV=\"development\" run-s clean:views \"snowpack -- dev\"",
    "test": "echo \"This template does not include a test runner by default.\" && exit 1",
    "update:content": "run-s clean:views clean:dist:views build:views",
    "build": "run-s build:*",
    "build:views": "cross-env NODE_ENV=\"production\" eleventy",
    "build:transpile": "cross-env NODE_ENV=\"production\" snowpack build",
    "build:bundle": "cross-env NODE_ENV=\"production\" rollup -c",
    "build:cleanup": "rimraf \"dist/web_modules\" \"dist/__snowpack__\"",
    "postinstall": "snowpack install",
    "clean": "run-p clean:*",
    "clean:views": "rimraf views",
    "clean:web_modules": "rimraf web_modules",
    "clean:dist": "rimraf \"dist\"",
    "clean:dist:views": "rimraf \"dist/**/*.html\" \"dist/content\"",
    "clean:dist:output": "rimraf \"dist/js\" \"dist/css\" \"dist/assets\" \"dist/*.txt\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" ",
    "serve:prod": "serve dist",
    "snowpack": "snowpack",
    "eleventy": "eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "@snowpack/plugin-run-script": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "liquidjs": "^9.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-import-url": "^5.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup-plugin-terser": "^7.0.0",
    "serve": "^11.3.2",
    "snowpack": "^2.7.7",
    "tailwindcss": "^1.6.2",
    "typescript": "^3.9.7",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "tiny-swiper": "^1.3.0"
  }
}
